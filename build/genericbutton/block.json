{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "portfolios/genericbutton",
  "title": "Portfolio Generic Button",
  "attributes": {
    "text": {
      "type": "string",
      "default": "Saiba Mais"
    },
    "size": {
      "type": "string",
      "default": "large"
    },
    "linkObject": {
